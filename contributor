[dashboard]
  title = Contributor Dashboard
  description = This dashboard gives an overview of own contributions.
  foreach = owner:self
[section "Mine To Rebase"]
  query = is:open -is:mergeable
[section "Mine To Rework"]
  query = is:open is:mergeable (label:Verified<=-1 OR label:Code-Review<=-1)
[section "Mine For Review"]
  query = is:open is:mergeable -(label:Verified<=-1 OR label:Code-Review<=-1)
[section "My Recently Merged"]
  query = is:merged limit:10
