[dashboard]
  title = Contributor Dashboard
  description = This dashboard gives an overview of own contributions.
  foreach = is:open owner:self
[section "Mine To Rebase"]
  query = -is:mergeable
[section "Mine To Rework"]
  query = is:mergeable (label:Verified<=-1 OR label:Code-Review<=-1)
[section "Mine For Review"]
  query = is:mergeable -(label:Verified<=-1 OR label:Code-Review<=-1)
