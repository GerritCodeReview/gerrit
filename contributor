[dashboard]
  title = Contributor Dashboard
  description = Own contributions.
  foreach = owner:self
[section "Mine To Rework"]
  query = is:open (label:Verified<=-1 OR label:Code-Review<=-1)
[section "Mine For Review"]
  query = is:open -(label:Verified<=-1 OR label:Code-Review<=-1)
[section "My Recently Merged"]
  query = is:merged limit:10
