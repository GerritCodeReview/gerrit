genrule(
  name = 'sourcecodepro',
  cmd = 'zip -rq $OUT .',
  srcs = ['sourcecodepro-regular.woff2'],
  out = 'sourcecodepro.zip',
  license = 'OFL1.1',
  visibility = ['PUBLIC'],
)

