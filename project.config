[project]
	description = Gerrit Code Review
	state = active
[receive]
	requireContributorAgreement = true
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/*"]
	owner = group gerrit-owners
[access "refs/heads/*"]
	push = group gerrit-owners
	label-Code-Review = -2..+2 group gerrit-maintainers
	label-Code-Review = -2..+2 group gerrit-owners
	label-Verified = -1..+1 group gerrit-maintainers
	label-Verified = -1..+1 group gerrit-owners
	label-Verified = -1..+1 group gerrit-verifiers
	submit = group gerrit-maintainers
	submit = group gerrit-owners
	create = group gerrit-maintainers
[access "refs/tags/*"]
	pushTag = group gerrit-maintainers
	pushTag = group gerrit-owners
[access]
	inheritFrom = Public-Projects
