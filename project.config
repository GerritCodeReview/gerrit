[project]
	description = Gerrit Code Review
	state = active
[receive]
	requireContributorAgreement = true
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/*"]
	owner = group tools-owners
[access "refs/heads/*"]
	push = group tools-owners
	label-Code-Review = -2..+2 group gerrit-submitters
	label-Code-Review = -2..+2 group tools-owners
	label-Verified = -1..+1 group gerrit-submitters
	label-Verified = -1..+1 group tools-gerrit-verifiers
	label-Verified = -1..+1 group tools-owners
	submit = group gerrit-submitters
	submit = group tools-owners
	create = group gerrit-submitters
[access "refs/tags/*"]
	pushTag = group gerrit-submitters
	pushTag = group tools-owners
