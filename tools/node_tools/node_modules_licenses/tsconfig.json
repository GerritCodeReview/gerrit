{
  "compilerOptions": {
    "plugins": [
      {
        "name": "@bazel/tsetse",
        "disabledRules": [
          "must-type-assert-json-parse"
        ]
      }
    ],
    "target": "es6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "outDir": "../../../.ts-out/tools/node_modules_licenses", // Not used in bazel,
    "types": ["node"]
  },
  "include": ["*.ts"]
}
